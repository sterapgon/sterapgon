                                    TMPCLS_F_DATE_M = ws12.Cells(t, "E").Value
                                    TMPCLS_F_DATE_Y = ws12.Cells(t, "F").Value
                                    TMPCLS_L_DATE_D = ws12.Cells(t, "G").Value
                                    TMPCLS_L_DATE_M = ws12.Cells(t, "H").Value
                                    TMPCLS_L_DATE_Y = ws12.Cells(t, "I").Value
                                    
