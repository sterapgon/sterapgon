                    Next n
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                    ' Process data from Sheet7 (BRANIC)
                    For o = 3 To lastRow7
                        ' Check if the NID in ws4 matches the current NID
                        If ws7.Cells(o, "A").Value = NID Then
                        
                                If Not AcroForm.getField("BRAINC_CHK") Is Nothing Then
                                    AcroForm.getField("BRAINC_CHK").Value = "Yes"
