Sub FillPDFWithMultipleSheet_PT09()
    ' Declare variables for Acrobat objects
    Dim AcroApp As Object
    Dim AcroAVDoc As Object
    Dim AcroPDDoc As Object
    Dim AcroForm As Object
    
    ' Declare worksheets for different sheets in the workbook
    Dim ws1 As Worksheet
    Dim ws2 As Worksheet
    Dim ws3 As Worksheet
    Dim ws4 As Worksheet
    Dim ws5 As Worksheet
    Dim ws6 As Worksheet
    Dim ws7 As Worksheet
    Dim ws8 As Worksheet
    Dim ws9 As Worksheet
    Dim ws10 As Worksheet
    Dim ws11 As Worksheet
    Dim ws12 As Worksheet
    Dim ws13 As Worksheet
    Dim ws14 As Worksheet
    Dim ws15 As Worksheet
    Dim ws16 As Worksheet
    
    ' Declare variables for the last row in each worksheet
    Dim lastRow1 As Long
    Dim lastRow2 As Long
    Dim lastRow3 As Long
    Dim lastRow4 As Long
    Dim lastRow5 As Long
    Dim lastRow6 As Long
    Dim lastRow7 As Long
    Dim lastRow8 As Long
    Dim lastRow9 As Long
    Dim lastRow10 As Long
    Dim lastRow11 As Long
    Dim lastRow12 As Long
    Dim lastRow13 As Long
    Dim lastRow14 As Long
    Dim lastRow15 As Long
    Dim lastRow16 As Long

    
    Dim i As Long, j As Long, k As Long, l As Long, m As Long
    Dim n As Long, o As Long, p As Long, q As Long, r As Long, s As Long
    Dim t As Long, u As Long, v As Long, w As Long, x As Long

    ' Variables for data from Sheet1 (NID DETAIL)
    Dim NID As String, TITLENAM As String, FIRNAM As String, LASNAM As String
    Dim LTOFLG As String, SUPOFFNAM As String, BRANAM_1 As String
    Dim BLDGNAM As String, ROOMNO As String, FLOORNO As String, VILLAGE As String
    Dim ADDNO As String, MOONO As String, SOINAM As String, YAEK As String, THNNAM As String
    Dim TAM_NAME As String, AMP_NAME As String, PROV_NAME As String, POSCOD As String
    Dim TELNO As String, EMAIL As String, WEBSITE As String, CHGCOD As String, NUMBER As String

    ' Variables for data from Sheet2 (DIS)
    Dim DISDAT_D As String, DISDAT_M As String, DISDAT_Y As String
    Dim CLSAUDNAM As String, CLSAUDTELNO As String
    Dim UPAINCAMO_DIS As String
    Dim ACCREBAMO_DIS As String
    Dim OTHASTAMO_DIS As String
    Dim ACCPABAMO_DIS As String
    
    ' Variables for data from Sheet3 (AMA)
    Dim NUMNIDAMA As String
    Dim AMANID As String
    Dim AMANAM As String
    Dim NEWAMANID As String
    Dim NEWAMANAM As String
    Dim AMA_DATE_D As String
    Dim AMA_DATE_M As String
    Dim AMA_DATE_Y As String
    Dim UPAINCAMO_AMA As String
    Dim ACCREBAMO_AMA As String
    Dim OTHASTAMO_AMA As String
    Dim ACCPABAMO_AMA As String
    
    ' Variables for data from Sheet4 (TRF)
    Dim TRF_DATE_D As String
    Dim TRF_DATE_M As String
    Dim TRF_DATE_Y As String
    Dim TRF_CHK As String
    Dim TRFNAM As String
    Dim TRFNID As String
    Dim UPAINCAMO_TRF As String
    Dim ACCREBAMO_TRF As String
    Dim OTHASTAMO_TRF As String
    Dim ACCPABAMO_TRF As String
    
    ' Variables for data from Sheet5 (PARTRF)
    Dim PARTRF_DATE_D As String
    Dim PARTRF_DATE_M As String
    Dim PARTRF_DATE_Y As String
    Dim PARTRF_CHK As String
    Dim PARTRFNAM As String
    Dim PARTRFNID As String
    
    ' Variables for data from Sheet6 (BYSTYPCHG)
    Dim BUSTYPCHG_1 As String, BUSTYPCHG_2 As String, BUSTYPCHG_3 As String
    Dim BUSTYPCHG_4 As String, BUSTYPCHG_5 As String, BUSTYPCHG_6 As String
    Dim BUSTYPCHG_7 As String, BUSTYPCHG_8 As String, BUSTYPCHG_9 As String, BUSTYPCHG_10 As String
    Dim BUSTYPCHG_DATE_D As String, BUSTYPCHG_DATE_M As String, BUSTYPCHG_DATE_Y As String
    
    ' Variables for data from Sheet7 (BRAINC)
    Dim NUMBRAINC As String
    Dim BRAINC_DATE_D As String
    Dim BRAINC_DATE_M As String
    Dim BRAINC_DATE_Y As String
    Dim BRANIC_BRANO As String
    Dim BRAINCNAM As String
    Dim BRAINC_BLDGNAM As String
    Dim BRANIC_ROOMNO As String
    Dim BRANIC_FLOORNO As String
    Dim BRANIC_VILLAGE As String
    Dim BRANIC_ADDNO As String
    Dim BRANIC_MOONO As String
    Dim BRANIC_SOINAM As String
    Dim BRANIC_YAEK As String
    Dim BRANIC_THNNAM As String
    Dim BRANIC_TAM_NAME As String
    Dim BRANIC_AMP_NAME As String
    Dim BRANIC_PROV_NAME As String
    Dim BRANIC_POSCOD As String
    Dim BRANIC_TELNO As String
    Dim BRANIC_EMAIL As String
    Dim BRANIC_BUSLOCTYP As String
    
    ' Variables for data from Sheet8 (BRADEC)
    Dim NUMBRADEC As String
    Dim BRADEC_DATE_D As String
    Dim BRADEC_DATE_M As String
    Dim BRADEC_DATE_Y As String
    Dim BRADEC_BRANO As String
    Dim BRADECNAM As String
    Dim BRADEC_BLDGNAM As String
    Dim BRADEC_ROOMNO As String
    Dim BRADEC_FLOORNO As String
    Dim BRADEC_VILLAGE As String
    Dim BRADEC_ADDNO As String
    Dim BRADEC_MOONO As String
    Dim BRADEC_SOINAM As String
    Dim BRADEC_YAEK As String
    Dim BRADEC_THNNAM As String
    Dim BRADEC_TAM_NAME As String
    Dim BRADEC_AMP_NAME As String
    Dim BRADEC_PROV_NAME As String
    Dim BRADEC_POSCOD As String
    
    ' Variables for data from Sheet9 (TRM)
    Dim TRM_DATE_D As String
    Dim TRM_DATE_M As String
    Dim TRM_DATE_Y As String
    Dim OLDTRMNAM As String
    Dim TRMOLDNID As String
    Dim NEWTRMNAM As String
    Dim TRMNEWNID As String
    
    ' Variables for data from Sheet10 (TXPNAMCHG)
    Dim TITFNAM As String
    Dim FIRNAM1 As String
    Dim MID1 As String
    Dim LASNAM1 As String
    Dim FIRNAM2 As String
    Dim TXPNAMCHG_DAT_D As String
    Dim TXPNAMCHG_DAT_M As String
    Dim TXPNAMCHG_DAT_Y As String
    Dim NEWTXPTITCOD As String
    Dim NEWTXPFIRNAM As String
    Dim NEWTXPMIDNAM As String
    Dim NEWTXPLASNAM As String
    Dim NEWJURNAM As String
    
    ' Variables for data from Sheet11 (BRANAMCHG)
    Dim BRANAM_11 As String
    Dim BRANO_11 As String
    Dim BRANAMCHG_DATE_D As String
    Dim BRANAMCHG_DATE_M As String
    Dim BRANAMCHG_DATE_Y As String
    Dim NEWBRANAM As String
    
    ' Variables for data from Sheet12 (TMPCLS)
    Dim BRANUM_12 As String
    Dim BRANO_12 As String
    Dim TMPCLS_F_DATE_D As String
    Dim TMPCLS_F_DATE_M As String
    Dim TMPCLS_F_DATE_Y As String
    Dim TMPCLS_L_DATE_D As String
    Dim TMPCLS_L_DATE_M As String
    Dim TMPCLS_L_DATE_Y As String

    ' Variables for data from Sheet13 (TXPDIE)
    Dim DIE_DATE_D As String
    Dim DIE_DATE_M As String
    Dim DIE_DATE_Y As String
    Dim TXPDIETRF As String
    
    ' Variables for data from Sheet14 (CHGOTHER)
    Dim OTHCHG_DATE_D As String
    Dim OTHCHG_DATE_M As String
    Dim OTHCHG_DATE_Y As String
    Dim OTHCHG_DETAIL As String
    Dim OTHCHG_DETAIL_NEW As String
    Dim OTHCHG_DETAIL_H As String
    
    ' Variables for data from Sheet15 (CHGAREALOC)
    Dim BRANO_15 As String
    Dim CHGAREALOC_DATE_D As String
    Dim CHGAREALOC_DATE_M As String
    Dim CHGAREALOC_DATE_Y As String
    Dim BLDGNAM_15 As String
    Dim ROOMNO_15 As String
    Dim FLOORNO_15 As String
    Dim VILLAGE_15 As String
    Dim ADDNO_15 As String
    Dim MOONO_15 As String
    Dim SOINAM_15 As String
    Dim YAEK_15 As String
    Dim THNNAM_15 As String
    Dim TAM_NAME_15 As String
    Dim AMP_NAME_15 As String
    Dim PROV_NAME_15 As String
    Dim POSCOD_15 As String
    Dim TELNO_15 As String
    Dim BLDGNAM_15_NEW As String
    Dim ROOMNO_15_NEW As String
    Dim FLOORNO_15_NEW As String
    Dim VILLAGE_15_NEW As String
    Dim ADDNO_15_NEW As String
    Dim MOONO_15_NEW As String
    Dim SOINAM_15_NEW As String
    Dim YAEK_15_NEW As String
    Dim THNNAM_15_NEW As String
    Dim TAM_NAME_15_NEW As String
    Dim AMP_NAME_15_NEW As String
    Dim PROV_NAME_15_NEW As String
    Dim POSCOD_15_NEW As String
    Dim TELNO_15_NEW As String
    
    ' Variables for data from Sheet16 (AREAMOV)
    Dim BRANO_16 As String
    Dim AREAMOV_D As String
    Dim AREAMOV_M As String
    Dim AREAMOV_Y As String
    Dim BLDGNAM_16 As String
    Dim ROOMNO_16 As String
    Dim FLOORNO_16 As String
    Dim VILLAGE_16 As String
    Dim ADDNO_16 As String
    Dim MOONO_16 As String
    Dim SOINAM_16 As String
    Dim YAEK_16 As String
    Dim THNNAM_16 As String
    Dim TAM_NAME_16 As String
    Dim AMP_NAME_16 As String
    Dim PROV_NAME_16 As String
    Dim POSCOD_16 As String
    Dim TELNO_16 As String
    Dim BLDGNAM_16_NEW As String
    Dim ROOMNO_16_NEW As String
    Dim FLOORNO_16_NEW As String
    Dim VILLAGE_16_NEW As String
    Dim ADDNO_16_NEW As String
    Dim MOONO_16_NEW As String
    Dim SOINAM_16_NEW As String
    Dim YAEK_16_NEW As String
    Dim THNNAM_16_NEW As String
    Dim TAM_NAME_16_NEW As String
    Dim AMP_NAME_16_NEW As String
    Dim PROV_NAME_16_NEW As String
    Dim POSCOD_16_NEW As String
    Dim TELNO_16_NEW As String
    Dim BUSLOCTYP_16_NEW As String
    Dim PP20REQ As String

    
    ' Variables for the PDF file paths
    Dim PDFPath As String
    Dim OutputPDF As String
    
    ' Log file for debugging
    Dim logFile As String
    logFile = ThisWorkbook.Path & "\logfile.txt"
    
    ' Store current date and time
    Dim currentDateTime As String
    currentDateTime = Format(Now, "yyyy-mm-dd hh:mm:ss")
    
    ' Create a Stream Object for writing the log file
    Dim objStream As Object
    Set objStream = CreateObject("ADODB.Stream")
    With objStream
        .Type = 2 ' Text data
        .Charset = "utf-8" ' For UTF-8 encoding
        .Open
    End With
    
    ' Initialize a counter for the number of PDFs created
    Dim pdfCount As Integer
    pdfCount = 0
    
    ' Set the worksheets to the respective sheet names
    Set ws1 = ThisWorkbook.Sheets("DATA NID")
    Set ws2 = ThisWorkbook.Sheets("DIS")
    Set ws3 = ThisWorkbook.Sheets("AMA")
    Set ws4 = ThisWorkbook.Sheets("TRF")
    Set ws5 = ThisWorkbook.Sheets("PARTRF")
    Set ws6 = ThisWorkbook.Sheets("BUSTYPCHG")
    Set ws7 = ThisWorkbook.Sheets("BRANIC")
    Set ws8 = ThisWorkbook.Sheets("BRADEC")
    Set ws9 = ThisWorkbook.Sheets("TRM")
    Set ws10 = ThisWorkbook.Sheets("TXPNAMCHG")
    Set ws11 = ThisWorkbook.Sheets("BRANAMCHG")
    Set ws12 = ThisWorkbook.Sheets("BUSTMPCLS")
    Set ws13 = ThisWorkbook.Sheets("TXPDIE")
    Set ws14 = ThisWorkbook.Sheets("CHGOTHER")
    Set ws15 = ThisWorkbook.Sheets("CHGAREALOC")
    Set ws16 = ThisWorkbook.Sheets("AREAMOV")
    
    ' Get the last row of data in each sheet
    lastRow1 = ws1.Cells(ws1.Rows.Count, "A").End(xlUp).Row
    lastRow2 = ws2.Cells(ws2.Rows.Count, "A").End(xlUp).Row
    lastRow3 = ws3.Cells(ws3.Rows.Count, "A").End(xlUp).Row
    lastRow4 = ws4.Cells(ws4.Rows.Count, "A").End(xlUp).Row
    lastRow5 = ws5.Cells(ws5.Rows.Count, "A").End(xlUp).Row
    lastRow6 = ws6.Cells(ws6.Rows.Count, "A").End(xlUp).Row
    lastRow7 = ws7.Cells(ws7.Rows.Count, "A").End(xlUp).Row
    lastRow8 = ws8.Cells(ws8.Rows.Count, "A").End(xlUp).Row
    lastRow9 = ws9.Cells(ws9.Rows.Count, "A").End(xlUp).Row
    lastRow10 = ws10.Cells(ws10.Rows.Count, "A").End(xlUp).Row
    lastRow11 = ws11.Cells(ws11.Rows.Count, "A").End(xlUp).Row
    lastRow12 = ws12.Cells(ws12.Rows.Count, "A").End(xlUp).Row
    lastRow13 = ws13.Cells(ws13.Rows.Count, "A").End(xlUp).Row
    lastRow14 = ws14.Cells(ws14.Rows.Count, "A").End(xlUp).Row
    lastRow15 = ws15.Cells(ws15.Rows.Count, "A").End(xlUp).Row
    lastRow16 = ws16.Cells(ws16.Rows.Count, "A").End(xlUp).Row
    
    ' Initialize the Acrobat application
    Set AcroApp = CreateObject("AcroExch.App")

    ' Loop through all records in Sheet1
    For i = 3 To lastRow1
        ' Retrieve data from Sheet1 (NID DETAIL)
        NID = ws1.Cells(i, "A").Value
        CHGCOD = ws1.Cells(i, "B").Value
        NUMBER = ws1.Cells(i, "C").Value
        TITLENAM = ws1.Cells(i, "D").Value
        FIRNAM = ws1.Cells(i, "F").Value
        LASNAM = ws1.Cells(i, "H").Value
        LTOFLG = ws1.Cells(i, "I").Value
        SUPOFFNAM = ws1.Cells(i, "W").Value
        BRANAM_1 = ws1.Cells(i, "Q").Value & " " & ws1.Cells(i, "S").Value
        BLDGNAM = ws1.Cells(i, "Z").Value
        ROOMNO = ws1.Cells(i, "AA").Value
        FLOORNO = ws1.Cells(i, "AB").Value
        VILLAGE = ws1.Cells(i, "AC").Value
        ADDNO = ws1.Cells(i, "AD").Value
        MOONO = ws1.Cells(i, "AE").Value
        SOINAM = ws1.Cells(i, "AF").Value
        YAEK = ws1.Cells(i, "AG").Value
        THNNAM = ws1.Cells(i, "AH").Value
        TAM_NAME = ws1.Cells(i, "AJ").Value
        AMP_NAME = ws1.Cells(i, "AL").Value
        PROV_NAME = ws1.Cells(i, "AN").Value
        POSCOD = ws1.Cells(i, "AO").Value
        TELNO = ws1.Cells(i, "AP").Value
        EMAIL = ws1.Cells(i, "AQ").Value
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
        ' Path to the template PDF file
        PDFPath = ThisWorkbook.Path & "\PT09-PAPER.pdf"
        Set AcroAVDoc = CreateObject("AcroExch.AVDoc")
        
        ' Open the PDF template
        If AcroAVDoc.Open(PDFPath, "") Then
            Set AcroPDDoc = AcroAVDoc.GetPDDoc
            Set AcroForm = AcroPDDoc.GetJSObject
            
            ' Check if the form object was retrieved successfully
            If Not AcroForm Is Nothing Then
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
' Fill PDF fields with data from Sheet1
                If LTOFLG = 1 Then
                    If Not AcroForm.getField("LTO_CHK") Is Nothing Then
                        AcroForm.getField("LTO_CHK").Value = "Yes" ' Tick the checkbox for LTO
                    End If
                Else
                    If Not AcroForm.getField("SUP_CHK") Is Nothing Then
                        AcroForm.getField("SUP_CHK").Value = "Yes" ' Tick the checkbox for SUP
                        If Not AcroForm.getField("SUPOFFNAM") Is Nothing Then
                            AcroForm.getField("SUPOFFNAM").Value = SUPOFFNAM ' Fill the SUPOFFNAM field
                        End If
                    End If
                End If
                
                            
                ' Fill PDF fields for NID and FIRNAM
                If Not AcroForm.getField("NID") Is Nothing Then
                    AcroForm.getField("NID").Value = NID
                End If
                
                If Not AcroForm.getField("FIRNAM") Is Nothing Then
                    AcroForm.getField("FIRNAM").Value = TITLENAM & " " & FIRNAM & " " & LASNAM
                End If

                ' Fill additional address fields if they exist
                With AcroForm
                    If BRANAM_1 <> "" Then .getField("BRANAM").Value = BRANAM_1
                    If BLDGNAM <> "" Then .getField("BLDGNAM").Value = BLDGNAM
                    If ROOMNO <> "" Then .getField("ROOMNO").Value = ROOMNO
                    If FLOORNO <> "" Then .getField("FLOORNO").Value = FLOORNO
                    If VILLAGE <> "" Then .getField("VILLAGE").Value = VILLAGE
                    If ADDNO <> "" Then .getField("ADDNO").Value = ADDNO
                    If MOONO <> "" Then .getField("MOONO").Value = MOONO
                    If SOINAM <> "" Then .getField("SOINAM").Value = SOINAM
                    If YAEK <> "" Then .getField("YAEK").Value = YAEK
                    If THNNAM <> "" Then .getField("THNNAM").Value = THNNAM
                    If TAM_NAME <> "" Then .getField("TAM_NAME").Value = TAM_NAME
                    If AMP_NAME <> "" Then .getField("AMP_NAME").Value = AMP_NAME
                    If PROV_NAME <> "" Then .getField("PROV_NAME").Value = PROV_NAME
                    If POSCOD <> "" Then .getField("POSCOD").Value = POSCOD
                    If TELNO <> "" Then .getField("TELNO").Value = TELNO
                    If EMAIL <> "" Then .getField("EMAIL").Value = EMAIL
                    If WEBSITE <> "" Then .getField("WEBSITE").Value = WEBSITE
                End With
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                    ' Process data from Sheet2 (DIS)
                    For j = 3 To lastRow2
                        ' Check if the NID in Sheet2 matches the current NID being processed from Sheet2
                        If ws2.Cells(j, "A").Value = NID Then
                        
                                If Not AcroForm.getField("CLAMATRF_CHK") Is Nothing Then
                                    AcroForm.getField("CLAMATRF_CHK").Value = "Yes"
                                End If
                                
                            ' Retrieve data from Sheet2 for DIS fields
                            DISDAT_D = ws2.Cells(j, "B").Value
                            DISDAT_M = ws2.Cells(j, "C").Value
                            DISDAT_Y = ws2.Cells(j, "D").Value
                            CLSAUDPIN = ws2.Cells(j, "E").Value
                            CLSAUDNAM = ws2.Cells(j, "F").Value & " " & ws2.Cells(j, "G").Value & " " & ws2.Cells(j, "H") & " " & ws2.Cells(j, "I")
                            CLSAUDTELNO = ws2.Cells(j, "J").Value
                            UPAINCAMO_DIS = ws2.Cells(j, "K").Value
                            ACCREBAMO_DIS = ws2.Cells(j, "L").Value
                            OTHASTAMO_DIS = ws2.Cells(j, "M").Value
                            ACCPABAMO_DIS = ws2.Cells(j, "N").Value
                                        
                            ' Check if NID exists in ws2 and mark the checkbox "DIS_CHK"
                            If Not IsEmpty(ws2.Cells(j, "A").Value) Then
                                If Not AcroForm.getField("DIS_CHK") Is Nothing Then
                                    AcroForm.getField("DIS_CHK").Value = "Yes" ' Tick the checkbox if NID is found
                                End If
                            End If
                    
                            ' Fill the DIS date fields in the PDF
                            If Not AcroForm.getField("DIS_DATE.D") Is Nothing Then AcroForm.getField("DIS_DATE.D").Value = DISDAT_D
                            If Not AcroForm.getField("DIS_DATE.M") Is Nothing Then AcroForm.getField("DIS_DATE.M").Value = DISDAT_M
                            If Not AcroForm.getField("DIS_DATE.Y") Is Nothing Then AcroForm.getField("DIS_DATE.Y").Value = DISDAT_Y
                            
                            ' Fill the AUDITOR details in the PDF
                            If Not IsEmpty(CLSAUDNAM) And Not AcroForm.getField("CLSAUDNAM") Is Nothing Then AcroForm.getField("CLSAUDNAM").Value = CLSAUDNAM
                            If Not IsEmpty(CLSAUDPIN) And Not AcroForm.getField("CLSAUDPIN") Is Nothing Then AcroForm.getField("CLSAUDPIN").Value = CLSAUDPIN
                            If Not IsEmpty(CLSAUDTELNO) And Not AcroForm.getField("CLSAUDTELNO") Is Nothing Then AcroForm.getField("CLSAUDTELNO").Value = CLSAUDTELNO
                            
                            If Not IsEmpty(UPAINCAMO_DIS) And Not AcroForm.getField("UPAINCAMO") Is Nothing Then AcroForm.getField("UPAINCAMO").Value = Format(UPAINCAMO_DIS, "#,##0.00")
                            If Not IsEmpty(ACCREBAMO_DIS) And Not AcroForm.getField("ACCREBAMO") Is Nothing Then AcroForm.getField("ACCREBAMO").Value = Format(ACCREBAMO_DIS, "#,##0.00")
                            If Not IsEmpty(OTHASTAMO_DIS) And Not AcroForm.getField("OTHASTAMO") Is Nothing Then AcroForm.getField("OTHASTAMO").Value = Format(OTHASTAMO_DIS, "#,##0.00")
                            If Not IsEmpty(ACCPABAMO_DIS) And Not AcroForm.getField("ACCPABAMO") Is Nothing Then AcroForm.getField("ACCPABAMO").Value = Format(ACCPABAMO_DIS, "#,##0.00")
                            
                            ' Stop the loop once the matching NID is found and processed
                            Exit For
                        End If
                    Next j
                    
'---------------------------------------------------------------------------------------------------
                    ' Process data from Sheet3 (AMA)
                    For k = 3 To lastRow3
                        ' Check if the NID in Sheet3 matches the current NID being processed from Sheet1
                        If ws3.Cells(k, "A").Value = NID Then
                                
                                If Not AcroForm.getField("CLAMATRF_CHK") Is Nothing Then
                                    AcroForm.getField("CLAMATRF_CHK").Value = "Yes"
                                End If
                                
                            ' Retrieve data from Sheet3 for AMA fields
                            NUMNIDAMA = ws3.Cells(k, "B").Value
                            AMANID = ws3.Cells(k, "C").Value
                            AMANAM = ws3.Cells(k, "D").Value & " " & ws3.Cells(j, "E").Value
                            NEWAMANID = ws3.Cells(k, "F").Value
                            NEWAMANAM = ws3.Cells(k, "G").Value & " " & ws3.Cells(j, "H").Value
                            AMA_DATE_D = ws3.Cells(k, "I").Value
                            AMA_DATE_M = ws3.Cells(k, "J").Value
                            AMA_DATE_Y = ws3.Cells(k, "K").Value
                            UPAINCAMO_AMA = ws3.Cells(k, "L").Value
                            ACCREBAMO_AMA = ws3.Cells(k, "M").Value
                            OTHASTAMO_AMA = ws3.Cells(k, "N").Value
                            ACCPABAMO_AMA = ws3.Cells(k, "O").Value
                    
                            ' Check if NID exists in ws3 and mark the checkbox "AMA_CHK"
                            If Not IsEmpty(ws3.Cells(k, "A").Value) Then
                                If Not AcroForm.getField("AMA_CHK") Is Nothing Then
                                    AcroForm.getField("AMA_CHK").Value = "Yes" ' Tick the checkbox if NID is found
                                End If
                            End If
                    
                            ' Fill the AMA date fields in the PDF
                            If Not AcroForm.getField("AMA_DATE.D") Is Nothing Then AcroForm.getField("AMA_DATE.D").Value = AMA_DATE_D
                            If Not AcroForm.getField("AMA_DATE.M") Is Nothing Then AcroForm.getField("AMA_DATE.M").Value = AMA_DATE_M
                            If Not AcroForm.getField("AMA_DATE.Y") Is Nothing Then AcroForm.getField("AMA_DATE.Y").Value = AMA_DATE_Y
                            
                            ' Fill the remaining AMA fields in the PDF
                            If Not IsEmpty(NUMNIDAMA) And Not AcroForm.getField("NUMNIDAMA") Is Nothing Then AcroForm.getField("NUMNIDAMA").Value = NUMNIDAMA
                            If Not IsEmpty(AMANAM) And Not AcroForm.getField("AMANAM") Is Nothing Then AcroForm.getField("AMANAM").Value = AMANAM
                            If Not IsEmpty(AMANID) And Not AcroForm.getField("AMANID") Is Nothing Then AcroForm.getField("AMANID").Value = AMANID
                            If Not IsEmpty(NEWAMANID) And Not AcroForm.getField("NEWAMANID") Is Nothing Then AcroForm.getField("NEWAMANID").Value = NEWAMANID
                            If Not IsEmpty(NEWAMANAM) And Not AcroForm.getField("NEWAMANAM") Is Nothing Then AcroForm.getField("NEWAMANAM").Value = NEWAMANAM
                            
                            If Not IsEmpty(UPAINCAMO_AMA) And Not AcroForm.getField("UPAINCAMO") Is Nothing Then AcroForm.getField("UPAINCAMO").Value = Format(UPAINCAMO_AMA, "#,##0.00")
                            If Not IsEmpty(ACCREBAMO_AMA) And Not AcroForm.getField("ACCREBAMO") Is Nothing Then AcroForm.getField("ACCREBAMO").Value = Format(ACCREBAMO_AMA, "#,##0.00")
                            If Not IsEmpty(OTHASTAMO_AMA) And Not AcroForm.getField("OTHASTAMO") Is Nothing Then AcroForm.getField("OTHASTAMO").Value = Format(OTHASTAMO_AMA, "#,##0.00")
                            If Not IsEmpty(ACCPABAMO_AMA) And Not AcroForm.getField("ACCPABAMO") Is Nothing Then AcroForm.getField("ACCPABAMO").Value = Format(ACCPABAMO_AMA, "#,##0.00")
                            
                            ' Stop the loop once the matching NID is found and processed
                            Exit For
                        End If
                    Next k
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                    ' Process data from Sheet4 (TRF)
                    For l = 3 To lastRow4
                        ' Check if the NID in ws4 matches the current NID
                        If ws4.Cells(l, "A").Value = NID Then
                        
                                If Not AcroForm.getField("CLAMATRF_CHK") Is Nothing Then
                                    AcroForm.getField("CLAMATRF_CHK").Value = "Yes"
                                End If
                                
                            ' Assign values from Sheet4 to variables
                            TRF_DATE_D = ws4.Cells(l, "F").Value
                            TRF_DATE_M = ws4.Cells(l, "G").Value
                            TRF_DATE_Y = ws4.Cells(l, "H").Value
                            TRF_CHK = ws4.Cells(l, "B").Value
                            TRFNAM = ws4.Cells(l, "D").Value & " " & ws4.Cells(l, "E").Value
                            TRFNID = ws4.Cells(l, "C").Value
                            UPAINCAMO_TRF = ws4.Cells(l, "I").Value
                            ACCREBAMO_TRF = ws4.Cells(l, "J").Value
                            OTHASTAMO_TRF = ws4.Cells(l, "K").Value
                            ACCPABAMO_TRF = ws4.Cells(l, "L").Value
                    
                            ' Check if the field in ws4 is not empty before filling the form field
                            If Not IsEmpty(ws4.Cells(l, "A").Value) Then
                                ' If the TRF_CHK checkbox field exists in the PDF, check the box
                                If Not AcroForm.getField("TRF_CHK") Is Nothing Then
                                    AcroForm.getField("TRF_CHK").Value = "Yes" ' Tick the checkbox in the PDF
                                End If
                            End If
                    
                            ' Check if TRF_CHK is "T" or "F" and fill the appropriate checkbox in the form
                            If TRF_CHK = "T" Then
                                ' If the field for "TRF_CHK_T" exists, check the box
                                If Not AcroForm.getField("TRF_CHK_T") Is Nothing Then
                                    AcroForm.getField("TRF_CHK_T").Value = "Yes"
                                End If
                            ElseIf TRF_CHK = "F" Then
                                ' If the field for "TRF_CHK_F" exists, check the box
                                If Not AcroForm.getField("TRF_CHK_F") Is Nothing Then
                                    AcroForm.getField("TRF_CHK_F").Value = "Yes"
                                End If
                            End If
                    
                            ' Fill in the transfer date fields in the form
                            If Not AcroForm.getField("TRF_DATE.D") Is Nothing Then AcroForm.getField("TRF_DATE.D").Value = TRF_DATE_D
                            If Not AcroForm.getField("TRF_DATE.M") Is Nothing Then AcroForm.getField("TRF_DATE.M").Value = TRF_DATE_M
                            If Not AcroForm.getField("TRF_DATE.Y") Is Nothing Then AcroForm.getField("TRF_DATE.Y").Value = TRF_DATE_Y
                    
                            ' Fill in the transfer name if it's not empty and the form field exists
                            If Not IsEmpty(TRFNAM) And Not AcroForm.getField("TRFNAM") Is Nothing Then AcroForm.getField("TRFNAM").Value = TRFNAM
                            ' Fill in the transfer NID if it's not empty and the form field exists
                            If Not IsEmpty(TRFNID) And Not AcroForm.getField("TRFNID") Is Nothing Then AcroForm.getField("TRFNID").Value = TRFNID
                            
                            If Not IsEmpty(UPAINCAMO_TRF) And Not AcroForm.getField("UPAINCAMO") Is Nothing Then AcroForm.getField("UPAINCAMO").Value = Format(UPAINCAMO_TRF, "#,##0.00")
                            If Not IsEmpty(ACCREBAMO_TRF) And Not AcroForm.getField("ACCREBAMO") Is Nothing Then AcroForm.getField("ACCREBAMO").Value = Format(ACCREBAMO_TRF, "#,##0.00")
                            If Not IsEmpty(OTHASTAMO_TRF) And Not AcroForm.getField("OTHASTAMO") Is Nothing Then AcroForm.getField("OTHASTAMO").Value = Format(OTHASTAMO_TRF, "#,##0.00")
                            If Not IsEmpty(ACCPABAMO_TRF) And Not AcroForm.getField("ACCPABAMO") Is Nothing Then AcroForm.getField("ACCPABAMO").Value = Format(ACCPABAMO_TRF, "#,##0.00")
                    
                            ' Exit the loop once the relevant NID has been found and processed
                            Exit For
                        End If
                    Next l
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                    ' Process data from Sheet5 (PARTRF)
                    For m = 3 To lastRow5
                        ' Check if the NID in ws4 matches the current NID
                        If ws5.Cells(m, "A").Value = NID Then

                                If Not AcroForm.getField("PARTRFCOD_CHK") Is Nothing Then
                                    AcroForm.getField("PARTRFCOD_CHK").Value = "Yes"
                                End If
                                
                            ' Assign values from Sheet4 to variables
                            PARTRF_DATE_D = ws5.Cells(m, "F").Value
                            PARTRF_DATE_M = ws5.Cells(m, "G").Value
                            PARTRF_DATE_Y = ws5.Cells(m, "H").Value
                            PARTRF_CHK = ws5.Cells(m, "B").Value
                            PARTRFNAM = ws5.Cells(m, "D").Value & " " & ws5.Cells(m, "E").Value
                            PARTRFNID = ws5.Cells(m, "C").Value

                    
                            ' Check if PARTRF_CHK is "T" or "F" and fill the appropriate checkbox in the form
                            If PARTRF_CHK = "T" Then
                                ' If the field for "PARTRF_CHK_T" exists, check the box
                                If Not AcroForm.getField("PARTRF_CHK_T") Is Nothing Then
                                    AcroForm.getField("PARTRF_CHK_T").Value = "Yes"
                                End If
                            ElseIf PARTRF_CHK = "F" Then
                                ' If the field for "PARTRF_CHK_F" exists, check the box
                                If Not AcroForm.getField("PARTRF_CHK_F") Is Nothing Then
                                    AcroForm.getField("PARTRF_CHK_F").Value = "Yes"
                                End If
                            End If
                    
                            ' Fill in the transfer date fields in the form
                            If Not IsEmpty(PARTRF_DATE_D) And Not AcroForm.getField("PARTRFDAT_DATE.D") Is Nothing Then AcroForm.getField("PARTRFDAT_DATE.D").Value = PARTRF_DATE_D
                            If Not IsEmpty(PARTRF_DATE_M) And Not AcroForm.getField("PARTRFDAT_DATE.M") Is Nothing Then AcroForm.getField("PARTRFDAT_DATE.M").Value = PARTRF_DATE_M
                            If Not IsEmpty(PARTRF_DATE_Y) And Not AcroForm.getField("PARTRFDAT_DATE.Y") Is Nothing Then AcroForm.getField("PARTRFDAT_DATE.Y").Value = PARTRF_DATE_Y
                    
                            If Not IsEmpty(PARTRFNAM) And Not AcroForm.getField("PARTRFNAM") Is Nothing Then AcroForm.getField("PARTRFNAM").Value = PARTRFNAM
                            If Not IsEmpty(PARTRFNID) And Not AcroForm.getField("PARTRFNID") Is Nothing Then AcroForm.getField("PARTRFNID").Value = PARTRFNID
                            
                    
                            ' Exit the loop once the relevant NID has been found and processed
                            Exit For
                        End If
                    Next m
'------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                ' Process data from Sheet6 (BUSTYPCHG)
                    For n = 3 To lastRow6
                        ' Check if the NID in ws4 matches the current NID
                        If ws6.Cells(n, "A").Value = NID Then
                        
                                If Not AcroForm.getField("BUSTYPCHG_CHK") Is Nothing Then
                                    AcroForm.getField("BUSTYPCHG_CHK").Value = "Yes"
                                End If
                                
                        ' Assigning values from the worksheet to the variables as strings
                            BUSTYPCHG_1 = CStr(ws6.Cells(n, "B").Value)
                            BUSTYPCHG_2 = CStr(ws6.Cells(n, "C").Value)
                            BUSTYPCHG_3 = CStr(ws6.Cells(n, "D").Value)
                            BUSTYPCHG_4 = CStr(ws6.Cells(n, "E").Value)
                            BUSTYPCHG_5 = CStr(ws6.Cells(n, "F").Value)
                            BUSTYPCHG_6 = CStr(ws6.Cells(n, "G").Value)
                            BUSTYPCHG_7 = CStr(ws6.Cells(n, "H").Value)
                            BUSTYPCHG_8 = CStr(ws6.Cells(n, "I").Value)
                            BUSTYPCHG_9 = CStr(ws6.Cells(n, "J").Value)
                            BUSTYPCHG_10 = CStr(ws6.Cells(n, "K").Value)
                            BUSTYPCHG_10 = CStr(ws6.Cells(n, "K").Value)
                            BUSTYPCHG_10 = CStr(ws6.Cells(n, "K").Value)
                            BUSTYPCHG_10 = CStr(ws6.Cells(n, "K").Value)
                            BUSTYPCHG_DATE_D = CStr(ws6.Cells(n, "L").Value)
                            BUSTYPCHG_DATE_M = CStr(ws6.Cells(n, "M").Value)
                            BUSTYPCHG_DATE_Y = CStr(ws6.Cells(n, "N").Value)
                            
                            If Not IsEmpty(BUSTYPCHG_DATE_D) And Not AcroForm.getField("BUSTYPCHG_DATE_D") Is Nothing Then AcroForm.getField("BUSTYPCHG_DATE_D").Value = BUSTYPCHG_DATE_D
                            If Not IsEmpty(BUSTYPCHG_DATE_M) And Not AcroForm.getField("BUSTYPCHG_DATE_M") Is Nothing Then AcroForm.getField("BUSTYPCHG_DATE_M").Value = BUSTYPCHG_DATE_M
                            If Not IsEmpty(BUSTYPCHG_DATE_Y) And Not AcroForm.getField("BUSTYPCHG_DATE_Y") Is Nothing Then AcroForm.getField("BUSTYPCHG_DATE_Y").Value = BUSTYPCHG_DATE_Y

                            ' Check and update the form fields based on the values in the variables for BUSTYPCHG_1 to BUSTYPCHG_10
                            
                            ' BUSTYPCHG_1
                            If BUSTYPCHG_1 = "I" Then
                                If Not AcroForm.getField("BUSTYPCHG_INC_1") Is Nothing Then
